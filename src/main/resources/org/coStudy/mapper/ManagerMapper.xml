<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.coStudy.mapper.ManagerMapper">
	
	<insert id="insert" parameterType="org.coStudy.domain.ManagerVO">
		insert into manager
		values(
		seq_manager.NEXTVAL, #{manager_id}, #{manager_pw}
		)
	</insert>

	<select id="list" resultType="org.coStudy.domain.ManagerVO">
		select * from manager
	</select>
	
	<delete id="delete" parameterType="int">
		delete from manager where manager_no = #{manager_no}
	</delete>
	
	<select id="managerLogin" parameterType="org.coStudy.domain.LoginVO" resultType="org.coStudy.domain.ManagerVO">
		select *
		from manager where manager_id=#{userId} and manager_pw = #{userPw}
	</select>
</mapper>